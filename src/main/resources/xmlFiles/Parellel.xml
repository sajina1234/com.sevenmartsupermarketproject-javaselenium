<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="MultipleTesting" thread-count="5" parallel="tests">
	<listeners>
		<listener class-name="com.sevenmartsupermarket.listeners.ExtentReporter"></listener>
	</listeners>
	<test name="SmokeRun">
		<classes>
			<class name="com.sevenmartsupermarket.tests.LoginTest"></class>
			<class name="com.sevenmartsupermarket.tests.AdminUsersTest"></class>
			<class name="com.sevenmartsupermarket.tests.ManageDeliveryBoyTest"></class>
			<class name="com.sevenmartsupermarket.tests.ManageSliderTest"></class>
		</classes>
	</test>
	<test name="ParellelRun">
		<classes>
			<class name="com.sevenmartsupermarket.tests.ManageUsersTest"></class>
			<class name="com.sevenmartsupermarket.tests.PushNotificationTest"></class>
			<class name="com.sevenmartsupermarket.tests.HomePageTest"></class>
			<class name="com.sevenmartsupermarket.tests.ManageLocationTest"></class>
			<class name="com.sevenmartsupermarket.tests.ManagePagesTest"></class>

		</classes>
	</test>
</suite>