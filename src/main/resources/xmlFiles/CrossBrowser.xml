<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="CrossBrowserTesting" thread-count="5" parallel="tests">
	<listeners>
		<listener class-name="com.sevenmartsupermarket.listeners.ExtentReporter"></listener>
	</listeners>
	<test name="Executing In Chrome">
	<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="com.sevenmartsupermarket.tests.PushNotificationTest"></class>
			<class name="com.sevenmartsupermarket.tests.AdminUsersTest"></class>
		</classes>
	</test>
	<test name="Executing In Edge">
	<parameter name="browser" value="edge"></parameter>
		<classes>
			<class name="com.sevenmartsupermarket.tests.PushNotificationTest"></class>
			<class name="com.sevenmartsupermarket.tests.AdminUsersTest"></class>
		</classes>
	</test>
	<test name="Executing In FireFox">
	<parameter name="browser" value="firefox"></parameter>
		<classes>
			<class name="com.sevenmartsupermarket.tests.PushNotificationTest"></class>
			<class name="com.sevenmartsupermarket.tests.AdminUsersTest"></class>
		</classes>
	</test>
</suite>